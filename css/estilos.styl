	border-radius 0
	margin-bottom 0
	// height 51px
	position absolute
	left 0
	z-index 1000
	width 100%
	&-collapse
		background #125599
	.container
		.navbar-header
			.navbar-toggle
				.icon-bar
					background-color #fff
			.navbar-brand
				display none
		.navbar-collapse
