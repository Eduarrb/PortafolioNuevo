                                font-size 14px
                                line-height 1.3
                                font-weight 400
                                color #ffffff
                                margin 0
                                text-align left
                                margin 0 0 15px 0
                            .name
                                font-size 20px
                                // font-weight 700
                                margin 0
                                margin-bottom 10px
                            .title
                                font-size 14px
                                // font-weight 700
                                margin 0
                        .bubble:hover
                            transform scale(1.05,1.05)
                    li:nth-child(1)
                        animation-delay 0.2s
                    li:nth-child(2)
                        animation-delay 0.2s
                    li:nth-child(3)
                        animation-delay 0.2s
                    li:nth-child(4)
                        animation-delay 0.2s
                    .animated
                        animation-fill-mode both
                        -moz-animation-timing-function ease-in-out
                        animation-duration 1s
                    .odd
                        margin 80px 0 0 0
                        float left
                        .bubble
                            margin 0 0 0 30px
                            float left
                            &-arrow
                                left -10px
                                right auto
                                background-position right top
                            .date
                            .time
                                text-align left
                        .point
                            left -29px
                    .fadeInUp
                        animation-name fadeInUp
@keyframes fadeInUp {
  0% {
    opacity: 0;
    transform: translateY(20px); }
  100% {
    opacity: 1;
    transform: translateY(0); } }

