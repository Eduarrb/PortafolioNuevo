            padding-bottom 40px
            h2
                font-family 'Lato', sans-serif
                font-weight 700
                color #125599
        &_fila
            &_timeline
                float left
                width 100%
                padding 50px 0 42px 0
                background url(../img/timelinebg.png) center top repeat-y
                ul
                    float left
                    width 100%
                    list-style none
                    padding 0
                    li
                        float left
                        width 50%
                        position relative
                        opacity 0
                        // filter alpha(opacity=0)
                        .point
                            width 57px
                            height 57px
                            background url(../img/timeline-item.png)
                            position absolute
                            right -28px
                            top 4px
                        .bubble
                            float right
                            width 64.4%
                            margin 0 30px 0 0
                            padding 22px 32px 25px 30px
                            background #363b3f
                            position relative
                            border-radius 6px
                            box-shadow 1px 2px 2px 2px rgba(0,0,0,0.1)
                            transition all 0.2s linear, 1s
                            &-arrow
                                width 16px
                                height 25px
                                background url(../img/bubbles-arrow.png) left top no-repeat
                                position absolute
                                top 20px
                                right -10px
