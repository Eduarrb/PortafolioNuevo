.chart {
  position: relative;
  display: inline-block;
  width: 110px;
  height: 110px;
  margin-top: 50px;
  margin-bottom: 50px;
  text-align: center;
}
.chart canvas {
  position: absolute;
  top: 0;
  left: 0;
}
.percent {
  display: inline-block;
  line-height: 152px;
  z-index: 2;
  margin-left: 42%;
  font-size: 35px;
  // font-family 'Lato', sans-serif

}
.percent:after {
  content: '%';
  margin-left: 0.1em;
  font-size: .8em;
}
